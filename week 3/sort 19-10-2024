list1 = input("enter the list:")
list2=list(map(int,list1.split()))
sort1=sorted(list2)
print("sorted list",sort1)
max1=max(list2)
print("maximun value",max1)
min1=min(list2)
print("minimun value",min1)
sort1.reverse()
print("reverse",sort1)
count=len(list2)
print("length of the list",count)
#sum=sum(list)
print(sum(list2))
